<?xml version="1.0" ?>
<list xmlns="http://schemas.sulu.io/list-builder/list">
    <key>blacklist_items</key>

    <properties>
        <property
            name="id"
            visibility="no"
            searchability="yes"
            translation="sulu_admin.id"
        >
            <field-name>id</field-name>
            <entity-name>%sulu.model.blacklist_item.class%</entity-name>
        </property>

        <property
            name="pattern"
            visibility="always"
            searchability="yes"
            translation="sulu_community.pattern"
        >
            <field-name>pattern</field-name>
            <entity-name>%sulu.model.blacklist_item.class%</entity-name>
        </property>

        <property
            name="type"
            visibility="always"
            searchability="yes"
            translation="sulu_community.type"
        >
            <field-name>type</field-name>
            <entity-name>%sulu.model.blacklist_item.class%</entity-name>
        </property>
    </properties>
</list>
